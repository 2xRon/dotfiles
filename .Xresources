!! URxvt

!! General Settings
URxvt.intensityStyles:	false
!URxvt.foreground:	#EEEEEE
!URxvt.background:   [70]#000000
URxvt.depth:		32
URxvt.scrollBar:	false
URxvt.cursorColor:	magenta
URxvt.iso14755: false
URxvt.iso14755_52: false

!! Extensions
URxvt.perl-ext-common:	default,matcher
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1

! Fonts
!Inconsolata
URxvt.xftAntialias:     true
! *font:             xft:inconsolata-12:antialias=true:hinting=true, xft:Noto Color Emoji,xft:Dejavu Sans Mono
*font:  xft:FantasqueSansMono Nerd Font Mono:size=12, xft:Inconsolata Nerd Font Mono:size=12, xft:DejaVu Sans:size=12            

! hard contrast: *background: #1d2021
! *background: #282828
*background: #323234
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
