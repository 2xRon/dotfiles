!! URxvt

!! General Settings
URxvt.intensityStyles:	false
!URxvt.foreground:	#EEEEEE
!URxvt.background:   [70]#000000
URxvt.depth:		32
URxvt.scrollBar:	false
URxvt.cursorColor:	magenta
URxvt.iso14755: false
URxvt.iso14755_52: false

!! Extensions
URxvt.perl-ext-common:	default,matcher
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.url-launcher:	linkhandler
URxvt.matcher.button:	1

! Fonts
!Inconsolata
URxvt.xftAntialias:     true
*font:             xft:inconsolata-12:antialias=true:hinting=true, xft:Noto Color Emoji,xft:Dejavu Sans Mono

! hard contrast: *background: #1d2021
*background: [80]#282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

